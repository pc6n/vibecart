    "http-proxy-middleware": "^3.0.3",
    "msgpack-lite": "^0.1.26",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.7.2",
    "three": "^0.161.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "socket.io-client": "^4.8.1",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/pchristen/racingcart.git"
  },
  "author": "Patrick Christen",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/multiplayer/testSetup.js"
    ],
